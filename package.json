    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "aos": "^2.3.4",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.18.0",
    "framer-motion": "^12.16.0",
    "keen-slider": "^6.8.6",
    "multer": "^2.0.1",
    "nodemailer": "^7.0.5",
    "react": "^19.1.0",
    "react-3d-carousel": "^0.0.6",
    "react-dom": "^19.1.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.0.1",
    "react-intersection-observer": "^9.16.0",
